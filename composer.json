{
	"name": "david63/resetregdate",
	"type": "phpbb-extension",
	"description": "Adds an Admin option to reset a user's registration date.",
	"version": "3.1.0-RC1",
	"time": "2020-01-10",
	"license": "GPL-2.0-only",
	"authors": [{
			"name": "David Wood",
			"role": "Developer"
		}],
	"require": {
        "php": ">=5.4,<7.1",
		"phpbb/phpbb": ">=3.3.0",
        "composer/installers": "~1.0"
    },
	"extra": {
		"display-name": "[**] Reset registration date",
		"soft-require": {
			"phpbb/phpbb": ">=3.3.0"
    	},
		"version-check": {
        	"host": "version.phpbbdev.space",
        	"directory": "/versions",
        	"filename": "reset_reg_date_version_file.json",
			"ssl": false
    	}
	}
}
